{
  "name": "AnimeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean:pods": "cd ios && rm -rf pods && pod install",
    "clean:android": "cd android/app && rm -rf build && cd .. && ./gradlew clean",
    "clean:modules": "rm -rf node_modules && yarn",
    "android-shake": "adb shell input keyevent 82",
    "build:js:android": "react-native bundle --entry-file='index.js' --bundle-output='./android/app/src/main/assets/index.android.bundle' --dev=false --platform='android' --assets-dest='./android/app/src/main/res'",
    "build:android:apkDebug": "yarn build:js:android && cd android && ./gradlew assembleDebug",
    "build:android:apkRelease": "yarn build:js:android && cd android && ./gradlew app:assembleRelease"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "6.1.17",
    "@react-navigation/stack": "6.3.29",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^5.32.0",
    "ky": "^1.2.4",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.2.0",
    "react-native": "0.73.7",
    "react-native-bootsplash": "^5.5.3",
    "react-native-devsettings": "^1.0.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-feather": "^1.1.2",
    "react-native-gesture-handler": "2.16.0",
    "react-native-mmkv": "^2.10.1",
    "react-native-reanimated": "3.8.1",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "3.30.1",
    "react-native-svg": "^15.2.0",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^8.1.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
